# Referencing `1-Makefile`
# Explicit declaration of object files during compilation.
# Initializing variables with object files as values.

CC = gcc
OBJ = main.o school.o
SRC = main.c school.c

all: school
	./school

school: $(OBJ)
	$(CC) $(OBJ) -o school

main.o: main.c
	$(CC) -c main.c

school.o: school.c
	$(CC) -c school.c
