# Referencing `1-Makefile`
# Explicit declaration of object files during compilation.
# Initializing variables with object files and executable as values.

CC = gcc
OBJ = *.o
SRC = main.c school.c
NAME = school

all: $(NAME)
	./$(NAME)

$(NAME): $(OBJ)
	$(CC) $(OBJ) -o $(NAME)

%.o: %.c
	$(CC) -c $(SRC)
